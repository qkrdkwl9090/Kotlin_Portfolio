<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ingstargram">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".MasterApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".IngStargramUpload"></activity>
        <activity android:name=".IngStargramMyPostListActivity" />
        <activity android:name=".IngStargramUserInfo" />
        <activity android:name=".IngStargramPostListActivity" />
        <activity android:name=".loginActivity" />
        <activity android:name=".BelonActivity" />
        <activity android:name=".Itube_detail" />
        <activity android:name=".ITubeActivity" />
        <activity android:name=".Email_Signup_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>